<?xml version="1.0" encoding="UTF-8"?>
<includes>
	<include name="List_View_53">
		<param name="mainlabel">$VAR[MainListLabel]</param>
		<param name="sublabel">$VAR[SubListLabel]</param>
		<param name="thumblabel">$VAR[MainListLabel]</param>
		<param name="rightlabel">$VAR[RightListLabel]</param>
		<param name="rightlabel2">$VAR[RightListLabel2]</param>
		<definition>
			<control type="group">
				<include>Visible_OpenClose</include>
				<visible>Control.IsVisible(53)</visible>
				<control type="image">
					<left>1170</left>
					<right>0</right>
					<height>100%</height>
					<texture colordiffuse="background_diffuse">common/white.jpg</texture>
				</control>
				<control type="image">
					<left>1170</left>
					<width>1</width>
					<height>100%</height>
					<texture colordiffuse="ff2a2a2a">common/white.jpg</texture>
				</control>
				<!-- List -->
				<control id="53" type="panel">
					<visible>$EXP[ListViewVisible]</visible>
					<top>35</top>
					<width>1230</width>
					<height>1060</height>
					<onup>SetFocus(6666)</onup>
					<onright condition="Control.IsVisible(17)">SetFocus(17)</onright>
					<onright condition="!Control.IsVisible(17) + Control.IsVisible(7000)">SetFocus(7000)</onright>
					<onright condition="!Control.IsVisible(17) + !Control.IsVisible(7000)">SetFocus(6666)</onright>
					<ondown>noop</ondown>
					<onleft>900</onleft>
					<defaultcontrol always="true">0</defaultcontrol>
					<focusposition>1</focusposition>
					<orientation>vertical</orientation>
					<viewtype label="535">list</viewtype>
					<pagecontrol>17</pagecontrol>
					<scrolltime tween="sine" easing="out">400</scrolltime>
					<preloaditems>4</preloaditems>
					<oninfo condition="String.IsEqual(ListItem.DBType,set) + !String.StartsWith(ListItem.Path,plugin://plugin.video)">RunScript(script.skin.helper.service,action=textviewer,header='$ESCINFO[ListItem.Title]',message='$ESCINFO[Window(home).Property(SkinHelper.ListItem.ExtendedPlot)]')</oninfo>
					<itemlayout height="120" width="1102">
						<control type="group">
							<top>60</top>
							<left>70</left>
							<include content="ListLayout">
								<param name="id">53</param>
								<param name="mainlabel">$PARAM[mainlabel]</param>
								<param name="sublabel">$PARAM[sublabel]</param>
								<param name="rightlabel">$PARAM[rightlabel]</param>
								<param name="rightlabel2">$PARAM[rightlabel2]</param>
								<param name="fallbacklabel">$PARAM[fallbacklabel]</param>
							</include>
						</control>
					</itemlayout>
					<focusedlayout height="120" width="1102">
						<include>SetPropertyOnFocus</include>
						<control type="group">
							<top>60</top>
							<left>70</left>
							<include content="ListLayout">
								<param name="focused" value="true"/>
								<param name="id">53</param>
								<param name="mainlabel">$PARAM[mainlabel]</param>
								<param name="sublabel">$PARAM[sublabel]</param>
								<param name="rightlabel">$PARAM[rightlabel]</param>
								<param name="rightlabel2">$PARAM[rightlabel2]</param>
								<param name="fallbacklabel">$PARAM[fallbacklabel]</param>
							</include>
						</control>
					</focusedlayout>
				</control>
				<!-- scrollbar -->
				<control type="scrollbar" id="17">
					<left>1166</left>
					<top>58</top>
					<bottom>0</bottom>
					<width>10</width>
					<onup>SetFocus(6666)</onup>
					<onleft>53</onleft>
					<onright condition="Control.IsVisible(7000)">SetFocus(7000)</onright>
					<orientation>vertical</orientation>
					<showonepage>false</showonepage>
					<texturesliderbackground colordiffuse="ff3b3b3b">common/white.jpg</texturesliderbackground>
					<texturesliderbar colordiffuse="lightgrey">common/white.jpg</texturesliderbar>
					<texturesliderbarfocus colordiffuse="emby">common/white.jpg</texturesliderbarfocus>
				</control>
				<!-- Hidden scrollbar -->
				<control id="96" type="scrollbar">
					<left>-1200</left>
					<width>100</width>
					<height>1</height>
					<showonepage>false</showonepage>
					<orientation>vertical</orientation>
				</control>
				<!-- Thumb image -->
				<control type="group">
					<visible>String.IsEqual(ListItem.DBType,movie) | String.IsEqual(ListItem.DBType,set) | String.IsEqual(ListItem.DBType,tvshow) | String.IsEqual(ListItem.DBType,episode) | String.IsEqual(ListItem.DBType,season) | String.StartsWith(ListItem.Path,pvr://) | [String.StartsWith(Container.FolderPath,plugin://plugin.video.emby/) + String.Contains(Container.FolderPath,type=homevideos)] | [String.StartsWith(Container.FolderPath,plugin://plugin.video.embycon) + !String.EndsWith(ListItem.Icon,icon.png)]</visible>
					<right>90</right>
					<top>125</top>
					<width>590</width>
					<control type="group">
						<control type="image">
							<top>-2</top>
							<left>-2</left>
							<width>596</width>
							<height>338</height>
							<texture>items/shadow_wide_big.png</texture>
						</control>
						<control type="image">
							<width>590</width>
							<height>332</height>
							<texture colordiffuse="thumb_background_fallback" diffuse="items/mask_wide_big.png">common/white.jpg</texture>
							<aspectratio scalediffuse="false">stretch</aspectratio>
							<visible>String.IsEmpty(ListItem.Icon) | String.StartsWith(ListItem.Icon,Default)</visible>
						</control>
						<control type="image">
							<width>590</width>
							<height>332</height>
							<texture colordiffuse="dark_button" diffuse="items/mask_wide_big.png">common/white.jpg</texture>
							<aspectratio scalediffuse="false">stretch</aspectratio>
							<visible>!String.IsEmpty(ListItem.Icon) + !String.StartsWith(ListItem.Icon,Default)</visible>
						</control>
						<control type="textbox">
							<top>10</top>
							<left>10</left>
							<width>570</width>
							<height>312</height>
							<font>Light24</font>
							<textcolor>white</textcolor>
														<align>center</align>
							<aligny>center</aligny>
							<label>$PARAM[thumblabel]</label>
							<visible>String.IsEmpty(ListItem.Icon) | String.StartsWith(ListItem.Icon,Default)</visible>
						</control>
						<control type="image">
							<width>590</width>
							<height>332</height>
							<texture diffuse="items/mask_wide_db.png" background="true">$VAR[IconLandscape]</texture>
							<aspectratio scalediffuse="false">keep</aspectratio>
							<bordersize>20</bordersize>
							<visible>String.StartsWith(ListItem.Path,pvr://) + !String.StartsWith(ListItem.Icon,Default)</visible>
						</control>
						<control type="image">
							<width>590</width>
							<height>332</height>
							<texture diffuse="items/mask_wide_db.png" background="true">$VAR[IconLandscape]</texture>
							<aspectratio scalediffuse="false">keep</aspectratio>
							<visible>!String.StartsWith(ListItem.Path,pvr://) + !String.StartsWith(ListItem.Icon,Default)</visible>
						</control>
						<control type="group">
							<width>590</width>
							<height>332</height>
							<include>bubble_overlay</include>
							<include>bubble_play</include>
						</control>
						<include content="progress">
							<param name="top" value="328"/>
							<param name="width" value="590"/>
						</include>
						<include content="static_labels">
							<param name="top">348</param>
							<param name="width">590</param>
							<param name="mainlabel">$PARAM[mainlabel]</param>
							<param name="sublabel">$PARAM[sublabel]</param>
							<param name="fallbacklabel">$PARAM[fallbacklabel]</param>
						</include>
					</control>
					<control type="grouplist">
						<orientation>vertical</orientation>
						<top>425</top>
						<height>550</height>
						<align>top</align>
						<itemgap>00</itemgap>
						<width>590</width>
						<control type="textbox" id="97">
							<width>590</width>
							<height>470</height>
							<align>justify</align>
							<aligny>top</aligny>
							<label>$VAR[ListViewPlot]</label>
							<font>Light24</font>
														<autoscroll>false</autoscroll>
						</control>
						<control type="label">
							<width>auto</width>
							<align>center</align>
							<aligny>top</aligny>
							<font>Material38</font>
							<textcolor>panel_text</textcolor>
							<label>î—“</label>
							<visible>Container(97).HasNext</visible>
						</control>
					</control>
				</control>
				<control type="group">
					<visible>![String.IsEqual(ListItem.DBType,movie) | String.IsEqual(ListItem.DBType,set) | String.IsEqual(ListItem.DBType,tvshow) | String.IsEqual(ListItem.DBType,episode) | String.IsEqual(ListItem.DBType,season) | String.StartsWith(ListItem.Path,pvr://) | [String.StartsWith(Container.FolderPath,plugin://plugin.video.emby/) + String.Contains(Container.FolderPath,type=homevideos)] | [String.StartsWith(Container.FolderPath,plugin://plugin.video.embycon) + !String.EndsWith(ListItem.Icon,icon.png)]]</visible>
					<right>90</right>
					<top>125</top>
					<width>590</width>
					<control type="group">
						<control type="group">
							<centerleft>50%</centerleft>
							<width>332</width>
							<control type="image">
								<top>-2</top>
								<left>-2</left>
								<width>338</width>
								<height>338</height>
								<texture>items/shadow_square_big.png</texture>
							</control>
							<control type="image">
								<width>332</width>
								<height>332</height>
								<texture colordiffuse="thumb_background_fallback" diffuse="items/mask_square_big.png">common/white.jpg</texture>
								<aspectratio scalediffuse="false">stretch</aspectratio>
								<visible>String.IsEmpty(ListItem.Icon) | String.StartsWith(ListItem.Icon,Default)</visible>
							</control>
							<control type="image">
								<width>332</width>
								<height>332</height>
								<texture colordiffuse="dark_button" diffuse="items/mask_square_big.png">common/white.jpg</texture>
								<aspectratio scalediffuse="false">stretch</aspectratio>
								<visible>!String.IsEmpty(ListItem.Icon) + !String.StartsWith(ListItem.Icon,Default)</visible>
							</control>
							<control type="textbox">
								<top>10</top>
								<left>10</left>
								<width>312</width>
								<height>312</height>
								<font>Light24</font>
								<textcolor>white</textcolor>
																<align>center</align>
								<aligny>center</aligny>
								<label>$PARAM[thumblabel]</label>
								<visible>String.IsEmpty(ListItem.Icon) | String.StartsWith(ListItem.Icon,Default)</visible>
							</control>
							<control type="image">
								<width>332</width>
								<height>332</height>
								<texture diffuse="items/mask_square_big.png" background="true">$VAR[IconSquare]</texture>
								<aspectratio scalediffuse="false">scale</aspectratio>
							</control>
							<control type="group">
								<width>332</width>
								<height>332</height>
								<include>bubble_overlay</include>
								<include>bubble_play</include>
							</control>
							<include content="progress">
								<param name="top" value="328"/>
								<param name="width" value="332"/>
							</include>
						</control>
						<include content="static_labels">
							<param name="top">348</param>
							<param name="width">590</param>
							<param name="mainlabel">$PARAM[mainlabel]</param>
							<param name="sublabel">$PARAM[sublabel]</param>
							<param name="fallbacklabel">$PARAM[fallbacklabel]</param>
						</include>
					</control>
					<control type="grouplist">
						<orientation>vertical</orientation>
						<top>425</top>
						<height>550</height>
						<align>top</align>
						<itemgap>00</itemgap>
						<width>590</width>
						<control type="textbox" id="97">
							<width>590</width>
							<height>470</height>
							<align>justify</align>
							<aligny>top</aligny>
							<label>$VAR[ListViewPlot]</label>
							<font>Light24</font>
														<autoscroll>false</autoscroll>
						</control>
						<control type="label">
							<width>auto</width>
							<align>center</align>
							<font>Material38</font>
							<textcolor>panel_text</textcolor>
							<label>î—“</label>
							<visible>Container(97).HasNext</visible>
						</control>
					</control>
				</control>
			</control>
		</definition>
	</include>
</includes>