<?xml version="1.0" encoding="UTF-8"?>
<window>
	<defaultcontrol always="true">50</defaultcontrol>
	<backgroundcolor>background</backgroundcolor>
	<views>50,52,53,</views>
	<menucontrol>6666</menucontrol>
	<include>UnloadSetProperties</include>
	<onload condition="!Window.Previous(fullscreenvideo) + !Window.Previous(visualisation) + !Skin.HasSetting(NoJumpToFirstPage)">action(firstpage)</onload>
	<controls>
		<control type="button" id="2">
			<include>HiddenObject</include>
		</control>
		<include>Background</include>
		<control type="group">
			<include>Window_OpenClose</include>
			<include>Wide_View_50</include>
			<include>Square_View_52</include>
			<include>List_View_53</include>
			<include>JumpToLetter_vertical</include>
		</control>
		<!-- scrollbar -->
		<include>ViewScrollbar</include>
		<!-- Menu control -->
		<control type="group">
			<visible>ControlGroup(4000).HasFocus</visible>
			<include>FullscreenDimensions</include>
			<include>Visible_PopupOpenClose</include>
			<include content="DiffuseMask">
				<param name="visible">true</param>
			</include>
			<control type="group">
				<centerleft>50%</centerleft>
				<width>384</width>
				<centertop>50%</centertop>
				<height>970</height>
				<control type="grouplist" id="4000">
					<orientation>vertical</orientation>
					<height>auto</height>
					<align>center</align>
					<itemgap>0</itemgap>
					<onright>50</onright>
					<onleft>50</onleft>
					<onback>50</onback>
					<usecontrolcoords>true</usecontrolcoords>
					<control type="image" id="4001">
						<width>100%</width>
						<height>10</height>
						<texture border="5" colordiffuse="panel">common/menu_top.png</texture>
					</control>
					<control type="togglebutton" id="4">
						<include content="CommonButton"/>
						<label>$LOCALIZE[580]: $LOCALIZE[584]</label>
						<altlabel>$LOCALIZE[580]: $LOCALIZE[585]</altlabel>
						<visible>Control.IsEnabled(4)</visible>
					</control>
					<control type="button" id="3">
						<include content="CommonButton"/>
						<label>$LOCALIZE[581]</label>
						<visible>Control.IsEnabled(3)</visible>
					</control>
					<control type="radiobutton" id="5">
						<include content="CommonButton"/>
						<label>$LOCALIZE[21340]</label>
						<visible>Control.IsEnabled(5)</visible>
					</control>
					<control type="radiobutton" id="6">
						<include content="CommonButton"/>
						<label>$LOCALIZE[25000]</label>
						<visible>Control.IsEnabled(6)</visible>
					</control>
					<control type="radiobutton" id="7">
						<include content="CommonButton"/>
						<label>$LOCALIZE[31133]</label>
						<visible>Control.IsEnabled(7)</visible>
					</control>
					<control type="radiobutton" id="8">
						<include content="CommonButton"/>
						<label>$LOCALIZE[31128]</label>
						<visible>Control.IsEnabled(8)</visible>
					</control>
					<control type="button" id="9">
						<include content="CommonButton"/>
						<label>$LOCALIZE[24034]</label>
						<visible>Control.IsEnabled(9)</visible>
					</control>
					<control type="image" id="4002">
						<width>100%</width>
						<height>10</height>
						<texture border="5" colordiffuse="panel">common/menu_bottom.png</texture>
					</control>
				</control>
			</control>
		</control>
		<!-- View select -->
		<include>ViewSelector</include>
		<!-- Download -->
		<control type="group">
			<visible>String.EndsWith(ListItem.Label2,&#x25;)</visible>
			<bottom>70</bottom>
			<animation type="Visible" reversible="false">
				<effect type="slide" start="0,200" end="0,0" delay="100" tween="circle" easin="out" time="400" />
				<effect type="fade" delay="160" end="100" time="400" />
			</animation>
			<animation type="Hidden" reversible="false">
				<effect type="slide" start="0,0" end="0,200" easing="in" tween="circle" easin="out" time="400" />
				<effect type="fade" start="100" end="0" time="400" />
			</animation>
			<width>350</width>
			<height>122</height>
			<left>70</left>
			<!-- Background image -->
			<control type="image">
				<texture colordiffuse="panel" border="10">common/dialog_back.png</texture>
			</control>
			<!-- icon -->
			<control type="group">
				<centertop>50%</centertop>
				<left>10</left>
				<width>80</width>
				<height>80</height>
				<!-- Icon back -->
				<control type="image">
					<texture>dialogs/progress/bg.png</texture>
				</control>
				<control type="image">
					<texture>dialogs/progress/100.png</texture>
					<colordiffuse>thumb_background</colordiffuse>
				</control>
				<!-- Icon -->
				<control type="textbox">
					<align>center</align>
					<aligny>center</aligny>
					<font>Material24</font>
					<label>î‹„</label>
				</control>
			</control>
			<!-- Details -->
			<control type="textbox">
				<top>20</top>
				<left>100</left>
				<right>20</right>
				<bottom>20</bottom>
				<font>Reg22</font>
				<scroll>true</scroll>
				<aligny>center</aligny>
				<label>$INFO[ListItem.Label2]</label>
			</control>
		</control>
		<!-- header bar -->
		<include content="HeaderBar">
			<param name="id">50</param>
			<param name="enabled">false</param>
			<param name="custom_animation">[[Control.IsVisible(54) + Container(540).HasPrevious]] | [Control.IsVisible(55) + Container(550).HasPrevious] | [Control.IsVisible(51) + [Integer.IsGreaterOrEqual(Container(51).CurrentItem,15) | Container(51).HasPrevious]] | [Control.IsVisible(50) + [Integer.IsGreaterOrEqual(Container(50).CurrentItem,13) | Container(50).HasPrevious]] | [Control.IsVisible(52) + [Integer.IsGreaterOrEqual(Container(52).CurrentItem,11) | Container(52).HasPrevious]]</param>
		</include>
		<!-- side panel -->
		<include content="SidePanel">
			<param name="onright">50</param>
		</include>
	</controls>
</window>
