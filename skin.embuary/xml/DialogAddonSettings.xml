<?xml version="1.0" encoding="UTF-8"?>
<window>
	<defaultcontrol always="true">9</defaultcontrol>
	<include>PropertyIncludes</include>
	<controls>
		<control type="label" id="20">
			<include>HiddenObject</include>
		</control>
		<include>Background</include>
		<control type="group">
			<include>Window_OpenClose</include>
			<!-- left -->
			<include content="ContentPanel">
			</include>
			<control type="group">
				<top>112</top>
				<include content="SideMenuHeadline">
					<param name="nr">1</param>
					<param name="label">$LOCALIZE[5]</param>
				</include>
				<control type="grouplist" id="9">
					<left>0</left>
					<top>55</top>
					<bottom>130</bottom>
					<width>400</width>
					<usecontrolcoords>true</usecontrolcoords>
					<onleft>900</onleft>
					<onright>2</onright>
					<onup>6666</onup>
					<ondown>1000</ondown>
				</control>
				<control type="button" id="13">
					<width>400</width>
					<height>50</height>
					<align>left</align>
					<aligny>center</aligny>
					<font>Reg22</font>
					<textoffsetx>50</textoffsetx>
					<textcolor>panel_text</textcolor>
					<focusedcolor>panel_text</focusedcolor>
					<selectedcolor>panel_text</selectedcolor>
					<texturefocus colordiffuse="panel_hover">common/white.jpg</texturefocus>
					<include>NoTextures</include>
				</control>
			</control>
		</control>
		<!-- right -->
		<control type="group">
			<include>Window_OpenClose</include>
			<top>122</top>
			<bottom>122</bottom>
			<control type="group">
				<left>400</left>
				<right>0</right>
				<bottom>0</bottom>
				<control type="image">
					<top>-1</top>
					<height>1</height>
					<texture colordiffuse="panel_hover">common/white.jpg</texture>
					<aspectratio>stretch</aspectratio>
				</control>
				<control type="image">
					<bottom>-1</bottom>
					<height>1</height>
					<texture colordiffuse="panel_hover">common/white.jpg</texture>
					<aspectratio>stretch</aspectratio>
				</control>
			</control>
			<control type="textbox">
				<bottom>-96</bottom>
				<left>500</left>
				<right>100</right>
				<height>76</height>
				<font>reg22</font>
			</control>
			<!-- Buttons -->
			<control type="grouplist" id="1000">
				<bottom>-60</bottom>
				<left>500</left>
				<right>50</right>
				<height>50</height>
				<orientation>horizontal</orientation>
				<itemgap>5</itemgap>
				<align>left</align>
				<onright>noop</onright>
				<onleft>9</onleft>
				<onup>2</onup>
				<control type="button" id="10">
					<include>DarkGreyButton</include>
					<onup>SetFocus(2)</onup>
				</control>
				<control type="button" id="11">
					<include>DarkGreyButton</include>
					<onup>SetFocus(2)</onup>
				</control>
				<control type="button" id="12">
					<include>DarkGreyButton</include>
					<onup>SetFocus(2)</onup>
				</control>
			</control>
			<!-- list -->
			<control type="grouplist" id="2">
				<left>500</left>
				<right>100</right>
				<height>100%</height>
				<onup>6666</onup>
				<onleft>9</onleft>
				<onright>noop</onright>
				<ondown>1000</ondown>
				<pagecontrol>17</pagecontrol>
				<scrolltime>200</scrolltime>
				<itemgap>0</itemgap>
			</control>
			<control type="button" id="3">
				<description>Default Button</description>
				<include>SettingsButton</include>
			</control>
			<control type="radiobutton" id="4">
				<description>Default RadioButton</description>
				<include>SettingsButton</include>
			</control>
			<control type="spincontrolex" id="5">
				<description>Default SpinControlex</description>
				<include>SettingsButton</include>
			</control>
			<control type="sliderex" id="8">
				<description>Default Slider</description>
				<include>SettingsButton</include>
			</control>
			<control type="image" id="6">
				<description>separator image</description>
				<visible>false</visible>
			</control>
			<control type="label" id="7">
				<description>Default Label</description>
				<height>70</height>
				<font>med22</font>
				<textoffsetx>0</textoffsetx>
				<textcolor>panel_text</textcolor>
				<aligny>center</aligny>
				<animation effect="slide" end="0,10" time="0" condition="true">Conditional</animation>
			</control>
		</control>
		<!-- scrollbar -->
		<include content="ViewScrollbar">
			<param name="visible" value="true"/>
			<param name="top" value="122"/>
			<param name="bottom" value="122"/>
			<param name="width" value="10"/>
		</include>
		<!-- header bar -->
		<include content="HeaderBar">
			<param name="id">9</param>
			<param name="auto_hide">false</param>
		</include>
		<!-- side panel -->
		<include content="SidePanel">
			<param name="onright">9</param>
		</include>
	</controls>
</window>
